generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id         Int       @default(autoincrement()) @id
  title      String
  released   String
  genre      String
  directory  String
  userId     Int
  createdAt  DateTime  @default(now())
  modifiedAt DateTime?
}